<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="import_products" instance="Limesharp\Import\Cron\Products\Products" method="execute">
            <schedule>30 * * * *</schedule>
        </job>
        <job name="import_stock" instance="Limesharp\Import\Cron\Stock\Stock" method="execute">
            <schedule>15 * * * *</schedule>
        </job>
    </group>
</config>
